$('#f-calendar').fullCalendar('removeEvents', [<%= @period.id %>]);
$('#f-calendar').fullCalendar(
  'renderEvent', 
  <%= raw render(partial: 'periods/period.json.jbuilder', locals: { period: @period }) %>, 
  true
);
$('#edit_event').modal('hide');
$(".fade.in").remove();
$('body').removeClass('modal-open');
$('.modal-backdrop').remove();
 
